{
  "name": "@rsc-parser/react-client",
  "version": "0.9.2",
  "license": "MIT",
  "packageManager": "yarn@4.8.1",
  "type": "module",
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint",
    "format": "prettier . --check --config ../../.prettierrc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "exports": {
    ".": {
      "import": "./dist/flight.js",
      "types": "./dist/flight.d.ts"
    }
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.24.0",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "eslint": "9.24.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0",
    "vite": "5.4.17",
    "vite-plugin-dts": "4.5.3"
  }
}

{
  "name": "@rsc-parser/react-client",
  "version": "0.9.1",
  "license": "MIT",
  "packageManager": "yarn@4.4.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint",
    "format": "prettier . --check --config ../../.prettierrc",
    "typecheck": "tsc --noEmit",

{
  "name": "@rsc-parser/core",
  "version": "0.9.2",
  "packageManager": "yarn@4.9.1",
  "author": "Alvar Lagerl√∂f",
  "license": "MIT",
  "scripts": {
    "build": "vite build && yarn dlx @tailwindcss/cli -o dist/style.css --minify",
    "dev": "concurrently  \"vite build --watch\" \"yarn dlx @tailwindcss/cli -o dist/style.css --minify --watch\"",
    "test": "NODE_NO_WARNINGS=1 node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --passWithNoTests",
    "test:dev": "yarn test --watch",
    "lint": "eslint",
    "format": "prettier . --check --config ../../.prettierrc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "type": "module",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css"
    },
    "./fetchPatcher": {
      "import": "./dist/fetchPatcher.js",
      "types": "./dist/fetchPatcher.d.ts"
    },
    "./readNextJsScriptTags": {
      "import": "./dist/readNextJsScriptTags.js",
      "types": "./dist/readNextJsScriptTags.d.ts"
    },
    "./events": {
      "import": "./dist/events.js",
      "types": "./dist/events.d.ts"
    }
  },
  "dependencies": {
    "@ariakit/react": "0.4.16",
    "@rsc-parser/react-client": "workspace:^",
    "d3": "7.9.0",
    "react-error-boundary": "5.0.0",
    "react-resizable-panels": "2.1.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.24.0",
    "@storybook/react": "8.6.12",
    "@tailwindcss/cli": "4.1.3",
    "@tailwindcss/postcss": "4.1.3",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/d3": "7.4.3",
    "@types/d3-drag": "3.0.7",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.29.1",
    "@typescript-eslint/parser": "8.29.1",
    "@vitejs/plugin-react": "4.3.4",
    "@vitejs/plugin-react-swc": "3.8.1",
    "concurrently": "9.1.2",
    "eslint": "9.24.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "4.1.3",
    "ts-jest": "29.3.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1",
    "vite": "5.4.17",
    "vite-plugin-dts": "4.5.3"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}

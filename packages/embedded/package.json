    ".": {
      "import": "./dist/js/embedded.js"
    },
    "./style.css": "./dist/style.css"
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@rsc-parser/core": "workspace:^",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.23.0",
    "rollup-plugin-preserve-directives": "0.4.0",
    "typescript": "5.8.3",
    "vite": "5.4.15",
    "vite-plugin-dts": "4.5.3"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }

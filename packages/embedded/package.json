{
  "name": "@rsc-parser/embedded",
  "packageManager": "yarn@4.8.1",
  "version": "0.9.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint",
    "format": "prettier . --check --config ../../.prettierrc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": [
    "package.json",
    "dist"
  ],
  "types": "./dist/js/RscDevtoolsPanel.d.ts",

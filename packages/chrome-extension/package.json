  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "@rsc-parser/core": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "web-vitals": "3.4.0",
    "zod": "3.21.4"

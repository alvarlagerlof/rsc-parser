{
  "name": "@rsc-parser/chrome-extension",
  "version": "0.9.0",
  "license": "MIT",
  "packageManager": "yarn@4.3.1",
  "private": true,
  "dependencies": {
    "@rsc-parser/core": "workspace:*",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "web-vitals": "4.2.2"
  },
  "devDependencies": {
    "@ariakit/react": "0.4.7",
    "@eslint/js": "9.7.0",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.16.1",
    "@vitejs/plugin-react": "4.3.1",
    "autoprefixer": "10.4.19",
    "chrome-types": "0.1.295",
    "concurrently": "8.2.2",
    "esbuild": "0.23.0",
    "eslint": "9.7.0",
    "postcss": "8.4.40",
    "prettier": "3.3.3",
    "type-fest": "4.22.1",
    "typescript": "5.5.4",
    "vite": "5.3.4",
    "vite-plugin-html-config": "1.0.11"
  },
  "scripts": {
    "lint": "eslint",
    "format": "prettier . --check --config ../../.prettierrc",
    "typecheck": "tsc --noEmit",
    "dev": "vite build --mode development && vite dev",
    "build": "vite build && sed -i'.backup' 's| http://localhost:6020||g' dist/manifest.json && rm dist/manifest.json.backup",
    "clean": "rm -rf dist"
  },
  "type": "module"
}

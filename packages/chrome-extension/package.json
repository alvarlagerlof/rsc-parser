{
  "name": "@rsc-parser/chrome-extension",
  "version": "0.9.2",
  "license": "MIT",
  "packageManager": "yarn@4.5.1",
  "private": true,
  "dependencies": {
    "@rsc-parser/core": "workspace:*",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.1.2",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@ariakit/react": "0.4.13",
    "@eslint/js": "9.14.0",
    "@types/eslint__js": "8.42.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "@vitejs/plugin-react": "4.3.3",
    "autoprefixer": "10.4.20",
    "chrome-types": "0.1.315",
    "concurrently": "9.0.1",
    "esbuild": "0.24.0",
    "eslint": "9.13.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "type-fest": "4.26.1",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-plugin-html-config": "2.0.2"
  },
  "scripts": {
    "lint": "eslint",
    "format": "prettier . --check --config ../../.prettierrc",
    "typecheck": "tsc --noEmit",
    "dev": "vite build --mode development && vite dev",
    "build": "vite build && sed -i'.backup' 's| http://localhost:6020||g' dist/manifest.json && rm dist/manifest.json.backup",
    "clean": "rm -rf dist"
  },
  "type": "module"
}

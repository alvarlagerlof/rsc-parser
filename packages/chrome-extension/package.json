{
  "name": "@rsc-parser/chrome-extension",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@3.6.3",
  "dependencies": {
    "@rsc-parser/core": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "web-vitals": "3.5.0",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@ariakit/react": "0.3.3",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.8",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitejs/plugin-react": "4.1.0",
    "autoprefixer": "10.4.16",
    "chrome-types": "0.1.231",
    "concurrently": "8.2.1",
    "esbuild": "0.19.4",
    "eslint": "8.50.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "type-fest": "4.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-html-config": "1.0.11"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern \"/dist\"/",
    "format": "prettier . --check --config ../../.prettierrc",
    "dev": "vite build --mode development && vite dev",
    "build": "vite build && sed -i'.backup' 's| http://localhost:6020||g' dist/manifest.json && rm dist/manifest.json.backup",
    "release": "zip -r dist.zip dist"
  },
  "type": "module"
}
